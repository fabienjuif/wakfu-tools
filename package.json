{
  "name": "@wakfu-tools/workspace",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "lint": "prettier --check 'src/**/*.{js,jsx}'",
    "publish-lambda": "node packages/cli publish"
  },
  "webDependencies": {},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "prettier": "^2.0.0",
    "rollup": "^2.11.2"
  },
  "workspaces": [
    "packages/*",
    "lambdas/*"
  ],
  "prettier": {
    "trailingComma": "all",
    "semi": false,
    "singleQuote": true,
    "arrowParens": "always",
    "endOfLine": "lf"
  }
}
